---
import Layout from "../layouts/Layout.astro";
import QuickPick from "../views/QuickPick.svelte";

const response = await fetch("http://localhost:8888/destination/pics")
  .then((res) => res.json())
  .then((data) => {
    return data["data"];
  });
---

<Layout title="Quick Pick">
  <QuickPick data={response} client:only/>
</Layout>

<style></style>
